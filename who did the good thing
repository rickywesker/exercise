#include<iostream>
using namespace std;
int main() {
	char thisman = 'A';
	bool none = true;
	while (thisman <= 'D') {
		if ((thisman != 'A') + (thisman == 'C') + (thisman == 'D') + (thisman != 'D') != 3) {
			thisman++;
			continue;
		}
		cout << thisman << " did it" << endl;
		none = false;
		break;
		
	}
	if (none) {
		cout << "no one did" << endl;
	}
	return 0;
}
